{
  "name": "vuex-nia",
  "version": "0.0.16",
  "description": "A state manager based on vuex and redux-saga.",
  "scripts": {
    "build:modern": "babel src/macro.js -o es5/macro.js && tsc --build tsconfig.es5.json",
    "build:es5": "babel src/macro.js -o macro.js && tsc --build tsconfig.json",
    "build": "npm run build:modern && npm run build:es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TerenceZ/nia.git"
  },
  "keywords": [
    "vuex",
    "redux-saga",
    "state-manager"
  ],
  "author": "TerenceZ",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.0.12",
    "typescript": "^3.5.2",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "peerDependencies": {
    "babel-plugin-macros": "^2.6.1",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "redux-saga": "^1.0.5",
    "vue-tsx.macro": "^0.3.5"
  }
}
